@model Movie

<div class="card">
  <div class="card-body">
    <h2 class="card-title">@Model.Name</h2>
    <p class="card-text">Duracion: @Model.Minutes</p>
    <p class="card-text">Review: @Model.Review</p>
    <p class="card-text">Codigo: @Model.Code</p>
    <p class="card-text">Categoria: @Model.Category</p>
  </div>
</div>